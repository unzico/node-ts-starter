{
  "name": "node-ts-starter",
  "version": "2.0.0",
  "description": "A NodeJS starter kit using TypeScript and Prettier.",
  "license": "MIT",
  "engines": {
    "node": ">=16.13.1",
    "yarn": "^3.1.1"
  },
  "scripts": {
    "audit": "yarn npm audit -A -R",
    "start": "node ./build/index.js",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "lint": "tsc --noEmit",
    "build": "yarn run build:tsc && yarn run build:compile",
    "build:tsc": "tsc",
    "build:compile": "babel ./src -d ./build -s --extensions \".js,.ts\" --ignore \"__tests__/*,__mocks__/*\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.5",
    "@babel/node": "7.18.5",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "7.18.0",
    "@babel/plugin-proposal-optional-chaining": "7.17.12",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "@babel/register": "7.17.7",
    "@types/node": "16.11.41",
    "babel-plugin-module-resolver": "4.1.0",
    "prettier": "2.7.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.6.4"
  },
  "packageManager": "yarn@3.2.1"
}
